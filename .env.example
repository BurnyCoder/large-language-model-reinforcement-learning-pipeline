# Real-time monitoring configuration
# Copy this file to .env and fill in your values

# Weights & Biases (https://wandb.ai)
# Get your API key from: https://wandb.ai/authorize
WANDB_API_KEY=your-wandb-api-key-here
WANDB_PROJECT=llmrl

# Neptune.ai (https://neptune.ai)
# Get your API token from: https://app.neptune.ai → Settings → API tokens
NEPTUNE_API_TOKEN=your-neptune-api-token-here
NEPTUNE_PROJECT=your-workspace/llmrl

# Training reporters (comma-separated: wandb,neptune,tensorboard)
# Default: wandb only
REPORT_TO=wandb,neptune,tensorboard
